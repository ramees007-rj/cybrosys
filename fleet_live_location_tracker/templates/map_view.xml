<odoo>
    <template id="fleet_location_map">
            <t t-set="title">Fleet | Live Location</t>
            <div class="oe_structure">
                <div class="container" id="fleet_location">
                        <iframe t-attf-src="https://www.google.com/maps/embed/v1/view?key={{API}}umg&amp;center={{latitude}},{{longitude}}&amp;zoom=16&amp;maptype=satellite" frameborder="0" width="100%" height="515px"/>
                </div>
            </div>
    </template>
</odoo>