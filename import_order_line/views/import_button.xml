<odoo>
    <record id="sale_order_import_order_lines_inherit" model="ir.ui.view">
        <field name="name">sale.order.import_order.lines.inherit</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_cancel']" position="after">
                <button name="action_import_order_lines"
                        string="Import Order Lines"
                        type="object"/>
            </xpath>
        </field>
    </record>
    <record id="college_exam_form" model="ir.ui.view">
        <field name="name">College Exam Form</field>
        <field name="model">xls.file</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="xls_file"/>
<!--                        <field name="sale_order_ref"/>-->
                    </group>
                </sheet>
                <footer>
                    <button name="action_import" type="object"
                            string="import"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

